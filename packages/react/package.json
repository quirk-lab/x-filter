{
  "name": "@x-filter/react",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "pnpm exec jest --selectProjects react",
    "lint": "pnpm exec eslint src --ext .ts,.tsx",
    "format": "pnpm exec prettier \"src/**/*.{ts,tsx}\" --check",
    "format:write": "pnpm exec prettier \"src/**/*.{ts,tsx}\" --write",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@x-filter/core": "workspace:*",
    "@x-filter/utils": "workspace:*"
  },
  "peerDependencies": {
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  }
}
