{
  "name": "@x-filter/core",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "test": "pnpm exec jest --selectProjects core",
    "lint": "pnpm exec eslint src --ext .ts",
    "format": "pnpm exec prettier \"src/**/*.ts\" --check",
    "format:write": "pnpm exec prettier \"src/**/*.ts\" --write",
    "typecheck": "tsc --noEmit"
  }
}
