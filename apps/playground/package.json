{
  "name": "@x-filter/playground",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "test": "pnpm exec jest --selectProjects playground",
    "lint": "pnpm exec eslint src --ext .ts,.tsx",
    "format": "pnpm exec prettier \"src/**/*.{ts,tsx}\" --check",
    "format:write": "pnpm exec prettier \"src/**/*.{ts,tsx}\" --write",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@x-filter/core": "workspace:*",
    "@x-filter/utils": "workspace:*",
    "@x-filter/react": "workspace:*",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0"
  }
}
